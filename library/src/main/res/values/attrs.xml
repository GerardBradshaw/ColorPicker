<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AbstractColorPickerView" >
        <attr name="enablePreview" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CompactColorPickerView" >
        <attr name="menuType" format="enum">
            <enum name="text" value="0" />
            <enum name="image" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RadialColorPickerView" >
        <attr name="snapThumbToCentre" format="boolean"/>
    </declare-styleable>
</resources>